{
  "stats":{},
  "log": {
    "access": "/var/log/v2ray/access.log",
    "error": "/var/log/v2ray/error.log",
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 1310,
      "tag": "app",
      "protocol": "vmess",
      "settings": {
        "clients": [

        ]
      },
      "streamSettings": {
        "network": "ws",
        "_security": "tls",
        "wsSettings": {
          "path": "/ws"
        }
      },
      "mux": {
        "enabled": true
      }
    },
    {
        "listen": "0.0.0.0",
        "port": 8080,
        "protocol": "dokodemo-door",
        "settings": {
          "address": "0.0.0.0"
        },
        "tag": "api"
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "freedom"
    }
  ],
  "dns": {
    "servers": [
      "8.8.8.8",
      "8.8.4.4",
      "localhost"
    ]
  },
   "api": {
    "tag": "api",
    "services": [
      "StatsService",
      "HandlerService",
      "LoggerService"
    ]
  },
  "policy": {
    "levels": {
      "0": {
        "statsUserUplink": true,
        "statsUserDownlink": true
      }
    },
    "system": {
      "statsInboundUplink": true,
      "statsInboundDownlink": true,
      "statsOutboundUplink": true,
      "statsOutboundDownlink": true
    }
  },
  "routing": {
    "rules": [
      {
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api",
        "type": "field"
      }
    ],
    "domainStrategy": "AsIs"
  }
}

